{
  "buildCommand": null,
  "outputDirectory": ".",
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/css/:path*",
      "destination": "/src/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/src/js/:path*"
    },
    {
      "source": "/data/:path*",
      "destination": "/public/data/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/src/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    },
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        },
        {
          "key": "X-Cache-Note",
          "value": "1hr cache allows regenerated static data to propagate within reasonable time"
        }
      ]
    }
  ]
}
