{
  "name": "game-entertainment-index",
  "version": "2.5.0",
  "description": "Ranks NFL, CFB, and NBA games by entertainment value using win probability data",
  "author": "Matthew Silverman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/game-entertainment-index"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "scripts": {
    "generate": "node scripts/generate-static.js",
    "generate:nfl": "node scripts/generate-static.js --sport NFL --season 2025 --all",
    "generate:cfb": "node scripts/generate-static.js --sport CFB --season 2025 --all",
    "generate:nba": "node scripts/generate-static.js --sport NBA --season 2025 --all",
    "trigger": "node scripts/trigger-generate.js",
    "compare:canonical": "node scripts/compare-canonical-games.js",
    "lint": "eslint \"src/js/**/*.js\" \"api/**/*.js\" \"scripts/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\" \"api/**/*.js\" \"scripts/**/*.js\""
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "prettier": "^3.7.4"
  }
}
